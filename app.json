{
  "expo": {
    "name": "movieApp",
    "slug": "movieApp",
    "version": "1.0.0",
    "scheme": "movieApp",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gvrdhn00.movieApp"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "907f7610-e3a0-4ef9-a809-fa20820393f6"
      }
    },
    "owner": "gvrdhn00"
  }
}
